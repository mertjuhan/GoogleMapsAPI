@model Dizi[]

    <div class="row">
        @foreach (var item in Model)
        {
        <div class="col-md-4 mb-2 ">
            <div class="card h-100">
                <div class="card-img-top" src="data:image/jpeg;base64," alt="Card image cap">
                </div>     
                <div class="card-body ">
                    <h5 class="card-title alert alert-success">@item.Name</h5>
                    <p class="card-text alert alert-warning"><strong>Adres :</strong> @item.FormattedAddress</p>
                    
                        <ul class="list-group">
                            <li class="list-group-item bg-danger text-white"><strong>Koordinatlar :</strong> @item.Geometry.Location.Lat , @item.Geometry.Location.Lng</li>
                            <li class="list-group-item">Dapibus ac facilisis in</li>
                            <li class="list-group-item">Morbi leo risus</li>
                            <li class="list-group-item">Porta ac consectetur ac</li>
                            <li class="list-group-item">Vestibulum at eros</li>
                        </ul>
                    
                          </div>
                <div class="d-flex w-100 justify-content-center mb-2 p-1">
                    <a href="#" class="btn btn-outline-primary w-75">Go somewhere</a>
                          </div>
                        </div>
            </div>
        }
    </div>
